    "jwt-encode": "1.0.1",
    "libphonenumber-js": "1.11.18",
    "motion-v": "0.5.0",
    "pinia": "2.3.1",
    "reka-ui": "1.0.0-alpha.9",
    "tailwind-merge": "2.6.0",
    "vue": "3.5.13",
    "vue-i18n": "11.0.1",
    "vue-router": "4.5.0",
    "zod": "3.24.1"
  },
  "devDependencies": {
