    "@tailwindcss/vite": "4.0.13",
    "@tanstack/vue-query": "5.67.3",
    "@vueuse/core": "13.0.0",
    "@vueuse/router": "13.0.0",
    "@wisemen/vue-core": "1.0.0-alpha.33",
    "@wisemen/vue-core-auth": "2.0.1",
    "clsx": "2.1.1",
    "cva": "1.0.0-beta.1",
    "dayjs": "1.11.13",
    "formango": "3.0.0-alpha.7",
    "i18n-iso-countries": "7.14.0",
    "jwt-encode": "1.0.1",
    "libphonenumber-js": "1.12.5",
    "motion-v": "0.11.3",
    "pinia": "3.0.1",
    "reka-ui": "2.0.2",
    "tailwind-merge": "3.0.2",
    "vue": "3.5.13",
    "vue-highlight-words": "3.0.1",
    "vue-i18n": "11.1.2",
    "vue-router": "4.5.0",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "0.64.11",
    "@import-meta-env/cli": "0.7.3",
    "@import-meta-env/unplugin": "0.6.2",
    "@playwright/test": "1.51.0",
    "@tailwindcss/vite": "4.0.9",
    "@tanstack/vue-query-devtools": "5.67.3",
    "@tsconfig/node20": "20.1.4",
    "@types/jsdom": "21.1.7",
