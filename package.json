    "@types/jsdom": "21.1.7",
    "@types/jwt-encode": "1.0.3",
    "@types/node": "22.14.0",
    "@vitejs/plugin-vue": "5.2.3",
    "@vue/language-server": "2.2.8",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.7.0",
    "@wisemen/eslint-config-vue": "1.2.0-beta.1",
    "@wisemen/vue-core-generator": "1.1.0",
    "@wisemen/vue-core-query": "0.0.34",
    "autoprefixer": "10.4.21",
    "chalk": "5.4.1",
    "dotenv": "16.4.7",
    "eslint": "9.23.0",
