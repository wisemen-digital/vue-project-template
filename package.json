    "@sentry/vue": "9.11.0",
    "@tailwindcss/vite": "4.1.2",
    "@tanstack/vue-query": "5.71.5",
    "@vueuse/core": "13.0.0",
    "@vueuse/router": "13.0.0",
    "@wisemen/vue-core": "1.0.0-alpha.42",
    "@wisemen/vue-core-auth": "2.0.1",
    "@wisemen/vue-core-components": "1.3.0",
    "clsx": "2.1.1",
    "cva": "1.0.0-beta.1",
    "dayjs": "1.11.13",
    "formango": "3.0.0-alpha.7",
    "i18n-iso-countries": "7.14.0",
    "jwt-encode": "1.0.1",
    "libphonenumber-js": "1.12.6",
    "motion-v": "1.0.0-beta.1",
    "pinia": "3.0.1",
    "reka-ui": "2.2.0",
    "tailwind-merge": "3.1.0",
    "vue": "3.5.13",
    "vue-highlight-words": "3.0.1",
    "vue-i18n": "12.0.0-alpha.1",
    "vue-router": "4.5.0",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "0.66.2",
