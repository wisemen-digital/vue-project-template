    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.7.0",
    "@wisemen/eslint-config-vue": "1.0.0-next.0",
    "@wisemen/vue-core-query": "0.0.33",
    "autoprefixer": "10.4.21",
    "chalk": "5.4.1",
    "dotenv": "16.4.7",
    "eslint": "9.22.0",
    "eslint-plugin-playwright": "2.2.0",
    "husky": "9.1.7",
    "inquirer": "12.4.3",
    "jsdom": "26.0.0",
